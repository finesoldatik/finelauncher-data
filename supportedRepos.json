			"repository": "VoxelEngine-Cpp",
			"releases": [],
			"buildCommands": {
				"linux": [
					"mkdir -p build",
					"@build cmake -DCMAKE_BUILD_TYPE=Release ..",
					"@build cmake --build .",
					"build/VoxelEngine"
				],
				"win32": [
					"!mkdir build",
					"@build cmake -DCMAKE_BUILD_TYPE=Release -DVOXELENGINE_BUILD_WINDOWS_VCPKG=ON ..",
					"@build cmake --build . --config Release",
					"build/Release/VoxelEngine.exe"
				]
			}
		},
